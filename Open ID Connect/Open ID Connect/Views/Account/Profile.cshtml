@{
}
@{
    ViewData["Title"] = "Profile";
}

<h1>User Profile</h1>

<div>
    <h4>Claims</h4>
    <dl>
        @foreach (var claim in User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</div>